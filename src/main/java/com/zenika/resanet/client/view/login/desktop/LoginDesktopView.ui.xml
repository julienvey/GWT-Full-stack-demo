<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

  <g:HTMLPanel>
    <table>
      <tr>
        <td>Login :</td>
        <td>
          <g:TextBox ui:field="loginTextBox"/>
        </td>
      </tr>
      <tr>
        <td>Mot de passe</td>
        <td>
          <g:PasswordTextBox ui:field="passwordTextBox"/>
        </td>
      </tr>
    </table>
  </g:HTMLPanel>
</ui:UiBinder>